<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="ElPais Suite" parallel="tests" thread-count="5">
    <test name="Chrome">
        <parameter name="useBrowserStack" value="true"/>
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="tests.ElPaisTest"/>
        </classes>
    </test>
    <test name="Firefox"><parameter name="useBrowserStack" value="true"/><parameter name="browser" value="firefox"/><classes><class name="tests.ElPaisTest"/></classes></test>
    <test name="Edge"><parameter name="useBrowserStack" value="true"/><parameter name="browser" value="edge"/><classes><class name="tests.ElPaisTest"/></classes></test>
    <test name="iPhone"><parameter name="useBrowserStack" value="true"/><parameter name="browser" value="iphone"/><classes><class name="tests.ElPaisTest"/></classes></test>
    <test name="Pixel"><parameter name="useBrowserStack" value="true"/><parameter name="browser" value="pixel"/><classes><class name="tests.ElPaisTest"/></classes></test>
</suite>
